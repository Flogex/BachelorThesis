            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Make creator ids distinct" enabled="true">
              <stringProp name="scriptLanguage">java</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">String varPrefix = &quot;task1CreatorId_&quot;;
String matchNrName = varPrefix + &quot;matchNr&quot;;
int creatorCount = Integer.parseInt(vars.get(matchNrName));

int nextIndex = 2;

for (int i = 2; i &lt;= creatorCount; i++) {
    String currentVarName = varPrefix + i;
    String currentId = vars.get(currentVarName);
    vars.remove(currentVarName);

    boolean mustContinue = false;
	
    for (int j = 1; j &lt; i; j++) {
    	   String oldId = vars.get(varPrefix + j);
        if (currentId.equals(oldId)) {
            mustContinue = true;
            break;
        }
    }

    if (mustContinue) {
        continue;
    }

    String newVarName = varPrefix + nextIndex;
    nextIndex++;
    vars.put(newVarName, currentId);
}

vars.remove(matchNrName);
vars.put(matchNrName, String.valueOf(nextIndex - 1));</stringProp>
            </JSR223PostProcessor>
